    <selectionEntry id="563b-76dd-1c3f-551c" name="Sector Mechanicus" hidden="false" collective="false" type="upgrade">
      <profiles>
        <profile id="65c7-a897-7dc7-56e3" name="Improvised Harness" hidden="false" profileTypeId="7f60-0255-d11e-0718" profileTypeName="Shared Tactic">
          <profiles/>
          <rules/>
          <infoLinks/>
          <modifiers/>
          <characteristics>
            <characteristic name="CP" characteristicTypeId="9f7b-18bb-3fea-2a63" value="1"/>
            <characteristic name="Description" characteristicTypeId="9c0d-fb62-000d-3237" value="Use this Tactic after a model from your kill team has moved (or remained stationary) in the Movement phase, if they are standing on the gantry of a Ferratonic Incinerator and not within 1&quot; of any enemy models. Until the end of the battle round, you do not have to take Falling tests for that model. This effect ends if the model moves."/>
          </characteristics>
        </profile>
        <profile id="efe6-3e47-0ada-1103" name="No Smoke Without Fire" hidden="false" profileTypeId="7f60-0255-d11e-0718" profileTypeName="Shared Tactic">
          <profiles/>
          <rules/>
          <infoLinks/>
          <modifiers/>
          <characteristics>
            <characteristic name="CP" characteristicTypeId="9f7b-18bb-3fea-2a63" value="2"/>
            <characteristic name="Description" characteristicTypeId="9c0d-fb62-000d-3237" value="Use this Tactic at the end of the Movement phase if you have a friendly model who is neither shaken nor within 1&quot; of any enemy models and who is within 1&quot; of the door of a Ferratonic Incinerator. In the subsequent Shooting phase, any attacks that target a model that is within 1&quot; of that Ferratonic Incinerator treat that model as obscured."/>
          </characteristics>
        </profile>
        <profile id="8176-75e7-5aeb-3cec" name="Taint the Air" hidden="false" profileTypeId="7f60-0255-d11e-0718" profileTypeName="Shared Tactic">
          <profiles/>
          <rules/>
          <infoLinks/>
          <modifiers/>
          <characteristics>
            <characteristic name="CP" characteristicTypeId="9f7b-18bb-3fea-2a63" value="2"/>
            <characteristic name="Description" characteristicTypeId="9c0d-fb62-000d-3237" value="Use this Tactic at the beginning of the battle round if you have a friendly model who is neither shaken nor within 1&quot; of any enemy models and is within 1&quot; of an Alchomite Stack. Until the end of the battle round, players add 1 to Injury rolls for models within 6&quot; of that Alchomite Stack."/>
          </characteristics>
        </profile>
        <profile id="1f31-f8bf-46d9-4e5a" name="Lethal Fumes" hidden="false" profileTypeId="7f60-0255-d11e-0718" profileTypeName="Shared Tactic">
          <profiles/>
          <rules/>
          <infoLinks/>
          <modifiers/>
          <characteristics>
            <characteristic name="CP" characteristicTypeId="9f7b-18bb-3fea-2a63" value="1"/>
            <characteristic name="Description" characteristicTypeId="9c0d-fb62-000d-3237" value="Use this tactic at the end of the Movement phase, if a model from your kill team is within 1&quot; of an Alchomite Stack and not within 1&quot; of any enemy models or shaken. Randomly determine one corner of the battle field. Draw a line between the Alchomite Stack and that corner, then roll a D6 for each model within 2&quot; of that line and 12&quot; ofthe Alchomite Stack - they suffer a mortal wound on a roll of 4+."/>
          </characteristics>
        </profile>
        <profile id="65e6-2f40-4119-53a5" name="Energy Sink" hidden="false" profileTypeId="7f60-0255-d11e-0718" profileTypeName="Shared Tactic">
          <profiles/>
          <rules/>
          <infoLinks/>
          <modifiers/>
          <characteristics>
            <characteristic name="CP" characteristicTypeId="9f7b-18bb-3fea-2a63" value="1"/>
            <characteristic name="Description" characteristicTypeId="9c0d-fb62-000d-3237" value="Use this tactic in the Shooting phase when you choose a model from your kill team to fire a weapon (or weapon profile) that wounds the bearer or takes them out of action on an unmodified hit roll of 1, and they are within 1&quot; of any Plasma Conduits. Until the end of the phase, unmodified hit rolls of 1 for this weapon will not wound the bearer or take them out of action."/>
          </characteristics>
        </profile>
        <profile id="825f-8893-5d27-7d17" name="Tap Conduits" hidden="false" profileTypeId="7f60-0255-d11e-0718" profileTypeName="Shared Tactic">
          <profiles/>
          <rules/>
          <infoLinks/>
          <modifiers/>
          <characteristics>
            <characteristic name="CP" characteristicTypeId="9f7b-18bb-3fea-2a63" value="1"/>
            <characteristic name="Description" characteristicTypeId="9c0d-fb62-000d-3237" value="Use this tactic in the Shooting phase when you choose a model from your kill team to fire a weapon (or weapon profile) that wounds the bearer or takes them out of action on an unmodified hit roll of 1, and they are within 1&quot; of any Plasma Conduits. Add 1 to the Damage of that weapon until the end of the phase."/>
          </characteristics>
        </profile>
        <profile id="a959-5ccc-d7c2-f7ac" name="Hit the Conduit" hidden="false" profileTypeId="7f60-0255-d11e-0718" profileTypeName="Shared Tactic">
          <profiles/>
          <rules/>
          <infoLinks/>
          <modifiers/>
          <characteristics>
            <characteristic name="CP" characteristicTypeId="9f7b-18bb-3fea-2a63" value="1"/>
            <characteristic name="Description" characteristicTypeId="9c0d-fb62-000d-3237" value="Use this Tactic when you choose a model from your kill team to shoot in the Shooting phase. If their target is within 1&quot; of a Plasma Conduit any wound rols of 6+ cause a mortal wound in addition to any other damage."/>
          </characteristics>
        </profile>
        <profile id="27ab-3fe8-fa86-0538" name="1 - Quiet Industry" hidden="false" profileTypeId="e89b-cbf0-2722-33fb" profileTypeName="Environment Table">
          <profiles/>
          <rules/>
          <infoLinks/>
          <modifiers/>
          <characteristics>
            <characteristic name="Result" characteristicTypeId="8ec8-e2f2-e3da-3854" value="No additional rule."/>
          </characteristics>
        </profile>
        <profile id="9d79-6577-5597-e1d8" name="2 - Artificial Quakes" hidden="false" profileTypeId="e89b-cbf0-2722-33fb" profileTypeName="Environment Table">
          <profiles/>
          <rules/>
          <infoLinks/>
          <modifiers/>
          <characteristics>
            <characteristic name="Result" characteristicTypeId="8ec8-e2f2-e3da-3854" value="Roll a D6 at the beginning of each battle round. On a roll of 1 or 2, all open ground is difficult terrain until the end of the battle round."/>
          </characteristics>
        </profile>
