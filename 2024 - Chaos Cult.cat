      <selectionEntries>
        <selectionEntry type="upgrade" import="true" name="Baleful Script" hidden="false" id="96fc-e7da-cac7-9229">
          <constraints>
            <constraint type="max" value="1" field="selections" scope="parent" shared="true" id="15f0-70df-077c-959a"/>
