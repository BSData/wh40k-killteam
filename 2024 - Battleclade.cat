        <selectionEntry type="upgrade" import="true" name="Covert Guises" hidden="false" id="3dbd-4fec-f265-31df">
          <constraints>
            <constraint type="max" value="1" field="selections" scope="parent" shared="true" id="59a1-84a4-7bd1-c80a"/>
